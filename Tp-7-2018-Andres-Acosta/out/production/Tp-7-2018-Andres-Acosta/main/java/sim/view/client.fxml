<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="panelClient" prefHeight="388.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.sim.controllers.ClientController">
   <children>
      <TableView fx:id="tvClient" layoutX="14.0" layoutY="15.0" prefHeight="304.0" prefWidth="585.0">
        <columns>
          <TableColumn fx:id="clientDate" prefWidth="54.0" text="Cliente" />
          <TableColumn fx:id="stateClient" prefWidth="81.0" text="Estado" />
            <TableColumn fx:id="inTime" prefWidth="149.0" text="Tiempo de entrada" />
            <TableColumn fx:id="inServerTime" prefWidth="154.0" text="Tiempo entrada a la Alfo..." />
            <TableColumn fx:id="outTime" minWidth="0.0" prefWidth="146.0" text="Tiempo de salida" />
        </columns>
      </TableView>
      <Button fx:id="btnExit" layoutX="537.0" layoutY="338.0" mnemonicParsing="false" onAction="#exitClient" text="Salir" />
      <TextField fx:id="txtFrom" layoutX="27.0" layoutY="348.0" prefHeight="25.0" prefWidth="57.0" />
      <TextField fx:id="txtTo" layoutX="104.0" layoutY="348.0" prefHeight="25.0" prefWidth="57.0" />
      <Label layoutX="42.0" layoutY="329.0" text="Desde" />
      <Label layoutX="119.0" layoutY="329.0" text="Hasta" />
      <Button layoutX="188.0" layoutY="348.0" mnemonicParsing="false" onAction="#searchClient" text="Buscar" />
   </children>
</AnchorPane>
