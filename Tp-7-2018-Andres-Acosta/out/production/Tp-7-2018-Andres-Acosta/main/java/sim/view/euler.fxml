<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="328.0" prefWidth="516.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.sim.controllers.EulerController">
   <children>
      <TableView fx:id="tvEuler" layoutX="34.0" layoutY="36.0" prefHeight="200.0" prefWidth="449.0">
        <columns>
          <TableColumn fx:id="step" prefWidth="75.0" text="N" />
          <TableColumn fx:id="time" prefWidth="75.0" text="M m" />
            <TableColumn fx:id="meters" prefWidth="75.0" text="T m" />
            <TableColumn fx:id="fuction" prefWidth="75.0" text="f(Mm;Tm)" />
            <TableColumn fx:id="timePlusOne" prefWidth="75.0" text="T m+1" />
            <TableColumn fx:id="meterPlusOne" prefWidth="75.0" text="M m+1" />
        </columns>
      </TableView>
      <Button fx:id="btnExit" layoutX="239.0" layoutY="265.0" mnemonicParsing="false" onAction="#exitEuler" text="Salir" />
   </children>
</AnchorPane>
